model calculated_values {
  id                                      Int       @id @default(autoincrement())
  nct_id                                  String?   @unique(map: "index_calculated_values_on_nct_id") @db.VarChar
  number_of_facilities                    Int?
  number_of_nsae_subjects                 Int?
  number_of_sae_subjects                  Int?
  registered_in_calendar_year             Int?
  nlm_download_date                       DateTime? @db.Date
  actual_duration                         Int?
  were_results_reported                   Boolean?  @default(false)
  months_to_report_results                Int?
  has_us_facility                         Boolean?
  has_single_facility                     Boolean?  @default(false)
  minimum_age_num                         Int?
  maximum_age_num                         Int?
  minimum_age_unit                        String?   @db.VarChar
  maximum_age_unit                        String?   @db.VarChar
  number_of_primary_outcomes_to_measure   Int?
  number_of_secondary_outcomes_to_measure Int?
  number_of_other_outcomes_to_measure     Int?
  studies                                 studies?  @relation(fields: [nct_id], references: [nct_id], onDelete: NoAction, onUpdate: NoAction) 

  @@index([nct_id], map: "calculated_values_nct_idx")
  @@index([actual_duration], map: "index_calculated_values_on_actual_duration")
  @@index([months_to_report_results], map: "index_calculated_values_on_months_to_report_results")
  @@index([number_of_facilities], map: "index_calculated_values_on_number_of_facilities")
}

