model outcome_analyses {
  id                          Int                       @id @default(autoincrement())
  nct_id                      String?                   @db.VarChar
  outcome_id                  Int?
  non_inferiority_type        String?                   @db.VarChar
  non_inferiority_description String?
  param_type                  String?                   @db.VarChar
  param_value                 Decimal?                  @db.Decimal
  dispersion_type             String?                   @db.VarChar
  dispersion_value            Decimal?                  @db.Decimal
  p_value_modifier            String?                   @db.VarChar
  p_value                     Float?
  ci_n_sides                  String?                   @db.VarChar
  ci_percent                  Decimal?                  @db.Decimal
  ci_lower_limit              Decimal?                  @db.Decimal
  ci_upper_limit              Decimal?                  @db.Decimal
  ci_upper_limit_na_comment   String?                   @db.VarChar
  p_value_description         String?                   @db.VarChar
