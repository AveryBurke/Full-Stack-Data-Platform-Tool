  ci_lower_limit_raw          String?                   @db.VarChar
  p_value_raw                 String?                   @db.VarChar
  studies                     studies?                  @relation(fields: [nct_id], references: [nct_id], onDelete: NoAction, onUpdate: NoAction) 
  outcomes                    outcomes?                 @relation(fields: [outcome_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
