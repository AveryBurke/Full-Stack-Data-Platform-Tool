  @@index([dispersion_type], map: "index_outcome_analyses_on_dispersion_type")
  @@index([nct_id], map: "index_outcome_analyses_on_nct_id")
  @@index([param_type], map: "index_outcome_analyses_on_param_type")
  @@index([nct_id], map: "outcome_analyses_nct_idx")
