generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model baseline_counts {
  id               Int            @id @default(autoincrement())
  nct_id           String?        @db.VarChar
  result_group_id  Int?
  ctgov_group_code String?        @db.VarChar
  units            String?        @db.VarChar
  scope            String?        @db.VarChar
  count            Int?
  studies          studies?       @relation(fields: [nct_id], references: [nct_id], onDelete: NoAction, onUpdate: NoAction) 
  result_groups    result_groups? @relation(fields: [result_group_id], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@index([nct_id], map: "baseline_counts_nct_idx")
  @@index([nct_id], map: "index_baseline_counts_on_nct_id")
}

model baseline_measurements {
  id                     Int            @id @default(autoincrement())
  nct_id                 String?        @db.VarChar
  result_group_id        Int?
  ctgov_group_code       String?        @db.VarChar
  classification         String?        @db.VarChar
  category               String?        @db.VarChar
  title                  String?        @db.VarChar
  description            String?
  units                  String?        @db.VarChar
  param_type             String?        @db.VarChar
  param_value            String?        @db.VarChar
  param_value_num        Decimal?       @db.Decimal
  dispersion_type        String?        @db.VarChar
  dispersion_value       String?        @db.VarChar
  dispersion_value_num   Decimal?       @db.Decimal
  dispersion_lower_limit Decimal?       @db.Decimal
  dispersion_upper_limit Decimal?       @db.Decimal
  explanation_of_na      String?        @db.VarChar
  number_analyzed        Int?
  number_analyzed_units  String?        @db.VarChar
  population_description String?        @db.VarChar
  calculate_percentage   String?        @db.VarChar
  studies                studies?       @relation(fields: [nct_id], references: [nct_id], onDelete: NoAction, onUpdate: NoAction) 
  result_groups          result_groups? @relation(fields: [result_group_id], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@index([nct_id], map: "baseline_measurements_nct_idx")
  @@index([category], map: "index_baseline_measurements_on_category")
  @@index([classification], map: "index_baseline_measurements_on_classification")
  @@index([dispersion_type], map: "index_baseline_measurements_on_dispersion_type")
  @@index([nct_id], map: "index_baseline_measurements_on_nct_id")
  @@index([param_type], map: "index_baseline_measurements_on_param_type")
}

model brief_summaries {
  id          Int      @id @default(autoincrement())
  nct_id      String?  @unique(map: "index_brief_summaries_on_nct_id") @db.VarChar
  description String?
  studies     studies? @relation(fields: [nct_id], references: [nct_id], onDelete: NoAction, onUpdate: NoAction) 

  @@index([nct_id], map: "brief_summaries_nct_idx")
}

